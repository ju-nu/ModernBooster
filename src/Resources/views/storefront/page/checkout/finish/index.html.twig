{% sw_extends '@Storefront/storefront/page/checkout/finish/index.html.twig' %}

{% block page_checkout_finish %}
    {% if config('JunuModernBooster.config.confettiEnabled') %}
        {% set bodyAttributes = bodyAttributes|merge({'data-confetti-enabled': 'true'}) %}
        <link rel="preload" href="https://cdn.jsdelivr.net/npm/canvas-confetti@1.9.3/dist/confetti.browser.min.js" as="script" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/canvas-confetti@1.9.3/dist/confetti.browser.min.js" async></script>
    {% endif %}
    {{ parent() }}
{% endblock %}

{% block page_checkout_additional_finish %}{% endblock %}